#include<stdio.h>
#include<stdlib.h>
typedef struct node{
      int data;
      struct node *next;
}NODE;
void main(){
     NODE*head= NULL;
    NODE *newnode,*temp;
    int choice=1;
    while(choice){
    newnode = (NODE*)malloc(sizeof(NODE));
    printf("enter data");
    scanf("%d",&newnode->data);
    newnode->next=NULL;
    if(head==NULL){
    head=temp=newnode;}
    else{
    temp->next=newnode;
    temp=newnode;
    }
    printf("do yoy want to continue 1,0");
    scanf("%d",&choice);
    }
choice=1;
while(choice){
newnode=(NODE*)malloc(sizeof(NODE));
printf("enter the element to insert");
scanf("%d",&newnode->data);
newnode->next=NULL;
temp=head;
    while (temp->next!=NULL){
        temp=temp->next;
    }
temp->next=newnode;
printf("more 1,stop 0");
scanf("%d",&choice);
}
temp=head;
    while (temp!=NULL){
        printf("%d ,",temp->data);
        temp=temp->next;
    }
}
